{
    "name": "http-cache",
    "version": "0.0.1",
    "description": "description",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "distribute": "npm run build && npm publish",
        "build": "tsc",
        "test": "jest",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "prepare": "husky install || exit 0 && npm run build ",
        "prepublish": "npm run build",
        "pre-commit": "pretty-quick --staged"
    },
    "homepage": "https://github.com/Avivbens/http-cache",
    "publisher": "Aviv Ben Shahar",
    "maintainers": [
        {
            "name": "Aviv Ben Shahar",
            "email": "avivbens87@gmail.com"
        }
    ],
    "keywords": [],
    "author": "Aviv Ben Shahar",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^29.0.0",
        "husky": "^7.0.4",
        "jest": "^29.0.0",
        "jest-cli": "^29.0.0",
        "prettier": "^2.3.2",
        "pretty-quick": "^3.1.3",
        "source-map-support": "^0.5.20",
        "ts-jest": "^29.0.0",
        "typescript": "^4.6.3"
    },
    "files": [
        "lib/**/*"
    ],
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node",
        "clearMocks": true
    },
    "dependencies": {
        "dexie": "^3.2.2",
        "rxjs": "^7.8.1"
    }
}
